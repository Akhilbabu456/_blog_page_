{
    "version":2,
    "builds": [
      { 
          "src": "package.json",
          "use": "@vercel/node" 
      }
    ],
   "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/login",
      "dest": "/loginform.js"
    },
    {
      "src": "/signup",
      "dest": "/signup.js"
    }
  ]
  }
